<style>
    .content{
        position: fixed;
        width: 100%;
        left: 50%;
        top: 50%;
        -webkit-transform: translate(-50%,-50%);
        -moz-transform: translate(-50%,-50%);
        -ms-transform: translate(-50%,-50%);
        -o-transform: translate(-50%,-50%);
        transform: translate(-50%,-50%);
    }
    .content .pic{
        text-align: center;
    }
    .content .pic img{
        width: 5.06rem;
        height: 2.78rem;
    }
    .content .tit{
        font-size:0.3rem;
        font-weight:400;
        color:#333;
        line-height:0.32rem;
        text-align: center;
        margin-top: 1rem;
    }
    .content .txt{
        font-size:0.24rem;
        font-weight:400;
        color:#999;
        line-height:0.32rem;
        text-align: center;
        margin-top:0.36rem;
        margin-bottom: 2.16rem;
    }
    .content .btn{
        display: block;
        width:2.60rem;
        height: 0.68rem;
        line-height: 0.68rem;
        text-align: center;
        font-size:0.3rem;
        background-color: #dd2534;
        color: #fff;
        -webkit-border-radius: 0.04rem;
        -moz-border-radius: 0.04rem;
        border-radius: 0.04rem;
        margin: 0 auto;
    }
    .content .btn.back{
        border: 1px solid #DD2534;
        background-color: transparent;
        color: #333;
        margin-bottom: 0.62rem;
    }

</style>


